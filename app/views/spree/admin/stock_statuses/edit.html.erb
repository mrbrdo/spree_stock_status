<% content_for :page_title do %>
  <%= link_to Spree.t(:stock_statuses), spree.admin_stock_statuses_path %> /
  <%= @stock_status.name %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @stock_status } %>

<%= form_for @stock_status, url: admin_stock_status_path(@stock_status) do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
