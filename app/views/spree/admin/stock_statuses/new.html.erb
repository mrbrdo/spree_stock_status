<% content_for :page_title do %>
  <%= link_to Spree.t(:stock_statuses), spree.admin_stock_statuses_path %> /
  <%= Spree.t(:new_stock_status) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @stock_status } %>

<%= form_for @stock_status, url: collection_url do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
